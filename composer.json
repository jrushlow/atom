{
  "name": "rushlow-development/atom",
  "description": "Create RSS/ATOM feeds using PHP.",
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://www.rushlow.dev",
  "authors": [
    {
      "name": "Jesse Rushlow",
      "email": "jr@rushlow.dev"
    }
  ],
  "require": {
    "php": ">=8.0",
    "league/uri": "^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.2",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-phpunit": "^1.0",
    "vimeo/psalm": "^4.14",
    "psalm/plugin-phpunit": "^0.16.1"
  },
  "autoload": {
    "psr-4": {
      "RushlowDevelopment\\Atom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RushlowDevelopment\\Atom\\": "tests"
    }
  }
